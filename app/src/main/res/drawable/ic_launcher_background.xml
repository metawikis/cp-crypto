<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="240dp"
    android:height="240dp"
    android:viewportWidth="240"
    android:viewportHeight="240"
    aapt:attr="resource/drawable/clown">

    <path
        android:name="shape"
        android:pathData="M0,0h240v240h-240z"
        android:fillType="evenOdd"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/purple_200"/>

    <path
        android:name="face"
        android:pathData="M120,120h-60v-60h60z"
        android:fill="white"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/black"/>

    <path
        android:name="nose"
        android:pathData="M120,100h-20v-10h20z"
        android:fill="red"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/black"/>

    <path
        android:name="eyes"
        android:pathData="M100,60h20v20h-20z"
        android:fill="black"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/black"/>

    <path
        android:name="smile"
        android:pathData="M100,100h20v-20h-20z"
        android:fill="white"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/black"/>

    <path
        android:name="hair"
        android:pathData="M120,120h-60v-60h60z"
        android:fill="yellow"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/black"/>

    <path
        android:name="hat"
        android:pathData="M120,120h-60v-60h60z"
        android:fill="red"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/black"/>

</vector>