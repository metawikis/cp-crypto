<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="240dp"
    android:height="240dp"
    android:viewportWidth="240"
    android:viewportHeight="240"
    aapt:attr="resource/drawable/fish">

    <path
        android:name="shape"
        android:pathData="M0,0h240v240h-240z"
        android:fillType="evenOdd"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/blue_200"/>

    <path
        android:name="body"
        android:pathData="M120,120h-60v-60h60z"
        android:fill="blue"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/black"/>

    <path
        android:name="eye"
        android:pathData="M100,60h20v20h-20z"
        android:fill="black"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/black"/>

    <path
        android:name="fin"
        android:pathData="M120,180h-20v-10h20z"
        android:fill="white"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/black"/>

    <path
        android:name="tail"
        android:pathData="M120,60h-20v-10h20z"
        android:fill="white"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:strokeMiterLimit="4"
        android:strokeWidth="2"
        android:strokeColor="@color/black"/>

</vector>
